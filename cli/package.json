{
  "name": "valist-cli",
  "description": "Valist CLI",
  "version": "0.0.4",
  "author": "Valist, Inc.",
  "bin": {
    "valist": "./bin/run"
  },
  "bugs": "https://github.com/valist-io/valist/issues",
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/valist-io/valist",
  "keywords": [
    "oclif"
  ],
  "license": "MPL-2.0",
  "main": "dist/index.js",
  "oclif": {
    "commands": "./dist/commands",
    "bin": "valist",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "account": {
        "description": "manage account keys"
      },
      "org": {
        "description": "manage organizations"
      }
    }
  },
  "repository": "https://github.com/valist-io/valist",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf dist && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "@truffle/hdwallet-provider": "^1.4.0",
    "@valist/sdk": "file:../lib",
    "js-yaml": "^4.1.0",
    "keytar": "^7.7.0",
    "reproducible": "0.0.3-rc.4"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.19",
    "@types/js-yaml": "^4.0.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.60",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "chai": "^4.3.4",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "eslint-plugin-import": "^2.23.4",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.10"
  }
}
