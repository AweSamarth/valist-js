{
  "name": "@valist/cli",
  "version": "0.0.1",
  "description": "Fetch and install versions of the valist-cli (GO).",
  "main": "dist/index.js",
  "bin": {
    "valist": "./bin/valist"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "postinstall": "node dist/index.js && chmod +x ./bin/valist",
    "build": "tsc",
    "dev": "tsc && node dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "dependencies": {
    "@valist/sdk": "^0.5.2",
    "axios": "^0.24.0",
    "progress": "^2.0.3"
  },
  "devDependencies": {
    "@types/progress": "^2.0.5",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "eslint": "^7.30.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.23.4",
    "typescript": "^4.3.5"
  },
  "keywords": [
    "valist",
    "ipfs",
    "secure-software"
  ],
  "author": "Valist, Inc.",
  "contributors": [
    "Alec M. Wantoch <alec@valist.io> (valist.io)",
    "Zachary J. Pelkey <zach@valist.io> (valist.io)",
